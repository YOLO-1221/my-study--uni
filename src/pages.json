{
	"pages": [
		{
			"path": "pages/style/index",
			"style": {
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/comp/index",
			"style": {
				"navigationStyle": "custom"
			}
		}
	],
	"subPackages": [
		{
			"root": "packageComp",
			"pages": [
				{
					"path": "comp1/index",
					"style": {
						"enablePullDownRefresh": true
					}
				}
			]
		},
		{
			"root": "packageStyle",
			"pages": [
				{
					"path": "style1/index",
					"style": {
						"enablePullDownRefresh": true
					}
				}
			]
		}
	],
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/style/index",
				"text": "样式"
			},
			{
				"pagePath": "pages/comp/index",
				"text": "组件"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
		"custom": {
			"^Com(.*)": "@/components/common/$1/$1.vue"
		}
	}
}